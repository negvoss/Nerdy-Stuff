{
  "manifest_version":2,
  "name":"Dictionary Box In Content Script",
  "version":"0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://morfix.co.il/",
    "http://www.wordreference.com/"
  ]
}
